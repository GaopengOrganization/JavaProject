<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
	
	<servlet>
		<servlet-name>contract</servlet-name>
		<servlet-class>com.hellojava.servlet.LSeekCont</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>laddcont</servlet-name>
		<servlet-class>com.hellojava.servlet.LAddCont</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>ship</servlet-name>
		<servlet-class>com.hellojava.servlet.LShipmentsServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>ship</servlet-name>
		<url-pattern>/ship</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>contract</servlet-name>
		<url-pattern>/cont</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>laddcont</servlet-name>
		<url-pattern>/lad</url-pattern>
	</servlet-mapping>
</web-app>
